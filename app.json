{
  "expo": {
    "name": "rn-app",
    "slug": "rn-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/shooting-start.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "githubUrl": "https://github.com/ivy-rong/rn-expo-app",
    "splash": {
      "image": "./assets/images/shooting-start.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/shooting-start.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.rnapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/shooting-start.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
